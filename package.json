{
  "name": "profe-blog",
  "version": "1.0.0",
  "description": "Sitio web profesional para Francisco Javier Núñez Valenzuela",
  "main": ".eleventy.js",
  "scripts": {
    "dev": "npx tailwindcss -i ./css/tailwind.css -o ./css/tw.css -m && npx @11ty/eleventy --serve",
    "start": "npm run dev",
    "build": "npx tailwindcss -i ./css/tailwind.css -o ./css/tw.css --minify && npx @11ty/eleventy",
    "preview": "npx http-server _site -p 8080",
    "deploy:gh-pages": "npx gh-pages -d _site",
    "deploy:netlify": "npx netlify deploy --prod --dir=_site",
    "convertir-dossieres": "node convertir-todos-dossieres-pdf.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@contentful/rich-text-html-renderer": "^16.3.5",
    "contentful": "^9.1.0",
    "cross-fetch": "^4.1.0",
    "dotenv": "^16.6.1",
    "firebase-admin": "^13.5.0",
    "mercadopago": "^2.0.15",
    "puppeteer": "^22.15.0",
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "gh-pages": "^5.0.0",
    "http-server": "^14.1.1",
    "netlify-cli": "^11.0.0"
  }
}
